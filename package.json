{
	"name": "he4rtbot-discord",
	"version": "1.0.0",
	"description": "Bot para o servidor heartdevs.com",
	"keywords": [
		"discord.js"
	],
	"homepage": "https://github.com/DanielHe4rt/He4rtBOT-Discord#readme",
	"bugs": {
		"url": "https://github.com/DanielHe4rt/He4rtBOT-Discord/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DanielHe4rt/He4rtBOT-Discord.git"
	},
	"license": "ISC",
	"author": "heartdevs",
	"main": "src/index.js",
	"scripts": {
		"dev": "nodemon src/index.js",
		"lint": "eslint --fix src/**/*.js",
		"lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
		"prettier": "prettier",
		"start": "node src/index.js",
		"start-windows": "node src/index.js"
	},
	"lint-staged": {
		"*.yml": [
			"yarn prettier --write",
			"git add"
		],
		"*.{ts,tsx,js}": [
			"yarn prettier --write",
			"yarn lint",
			"git add"
		],
		"package.json": [
			"yarn prettier --write",
			"yarn sort-package-json",
			"git add"
		]
	},
	"dependencies": {
		"@vitalets/google-translate-api": "^2.8.0",
		"axios": "^0.19.0",
		"discord.js": "^11.5.1",
		"dotenv": "^8.0.0",
		"enmap": "^5.1.0",
		"fs-extra": "^8.1.0",
		"is-image": "^3.0.0",
		"jimp": "^0.6.4",
		"lodash": "^4.17.15",
		"mathjs": "^6.0.3",
		"moment": "^2.24.0",
		"slot-machine": "^2.1.0",
		"snekfetch": "^4.0.4",
		"tmi.js": "^1.4.5",
		"twit": "^2.2.11"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"babel-eslint": "^10.0.2",
		"chalk": "^2.4.2",
		"eslint": "^6.1.0",
		"eslint-config-airbnb-base": "^13.2.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-prettier": "^3.1.0",
		"husky": "^4.2.1",
		"lint-staged": "^10.0.7",
		"nodemon": "^1.19.1",
		"prettier": "^1.18.2",
		"sort-package-json": "^1.39.1"
	}
}
